    "scripts": {
        "start": "vite",
        "dev": "yarn start",
        "start:mock": "VITE_REACT_APP_OFFLINE=true vite",
        "start:production": "serve --ssl-cert config/crt/dummy.pem --ssl-key config/crt/dummy.key -p ${PORT:-3010} build",
        "build": "vite build",
        "build:serve": "yarn build && yarn start:production",
        "build:serve:sourcemap": "yarn build --sourcemap true && yarn start:production",
        "test": "vitest",
        "test:ci": "vitest",
        "lint": "eslint .",
        "generate-index": "yarn ts-node scripts/generateIndex.ts > public/songs/index.json",
        "predeploy": "yarn generate-index && yarn build",
        "deploy": "gh-pages -d build",
        "prepare": "husky install",
        "e2e": "yarn playwright test",
        "e2e:prod": "PROD_RUN=true yarn playwright test",
        "test-ct": "yarn playwright test -c playwright-ct.config.ts",
        "peerjs": "yarn node peerJSServer.js",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "video-start": "remotion preview src/videos/index.ts --port 4000",
        "video-render": "remotion render src/videos/index.ts UpdateVideo out/video.mp4",
        "postinstall": "husky install"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
