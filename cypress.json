{
    "blockHosts": ["*.g.doubleclick.net", "*.ingest.sentry.io"],
    "chromeWebSecurity": false,
    "retries": {
        "runMode": 2,
        "openMode": 0
    },
    "video": false,
    "ignoreTestFiles": ["**/__snapshots__/*", "**/__image_snapshots__/*"],
    "env": {
        "cypress-plugin-snapshots": {
            "imageConfig": {
                "threshold": 0.1
            }
        }
    }
}
